<h1>All Merchants</h1>
<%= render partial: "flash" %>
<ul>
<% @merchants.each do |merchant| %>
  <li><%= link_to merchant.name, merchant_path(merchant) %><br>
  <%= link_to "Edit", edit_merchant_path(merchant) %> | <%= link_to "Delete", merchant_path(merchant), method: :delete %></li>
<% end %>
</ul>
<%= link_to "New Merchant", new_merchant_path %>
